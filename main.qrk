{
  # Taking first argument as file
  (let file (index std:args 0))
  (if (!= file none)
    {
      # Executing if file specified
      (let path (fs:join fs:cwd file))
      (std:run (fs:read path) path)
    }
    {
      # Else running REPL
      (let replPath (fs:join fs:root "sample" "repl.qrk"))
      (std:run (fs:read replPath) replPath)
    }
  )
}